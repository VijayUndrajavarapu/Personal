package craze;

public class superClas {

	public void adreas() {
		System.out.println("Parent class adreas method");
	}
	
	public class SuperSubClas extends superClas{
		public void adreas() {
			System.out.println("Child Class adreas method");
		}
	}
	
	public static void main(String[] args) {
		superClas sc = new superClas();
		sc.adreas();
		
	}

}
